                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">1.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                                            <fo:inline>Select and item in the center
                                                pane</fo:inline>
                                        </fo:block></fo:list-item-body></fo:list-item>
                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">2.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">
                                            <fo:block>Navigate to the "Related" tab
                                                and click on “click here”<fo:inline chfox:inch="true"><fo:inline>&#x2028;</fo:inline></fo:inline><fo:block-container height="186px" max-width="100%" width="823px"><fo:block end-indent="0" start-indent="0"><fo:inline><fo:external-graphic src="file:/C:/Users/Oliver/Downloads/Dita-Project-master/Corey/relating_items.png" max-width="100%" scaling="non-uniform" content-width="823px" content-height="186px"/></fo:inline></fo:block></fo:block-container><fo:inline chfox:inch="true"><fo:inline>&#x2028;</fo:inline></fo:inline></fo:block>
                                            <fo:block>A window will appear, prompting
                                                you to select a second item</fo:block>
                                        </fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">3.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                                            <fo:inline>Select a second item from the
                                                same library</fo:inline>
                                        </fo:block></fo:list-item-body></fo:list-item>
                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end">4.
            </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
